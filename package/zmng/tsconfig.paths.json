{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      //설정파일
      "@config/*": ["config/*"],

      //라우터
      "@router": ["Router.tsx"],

      //공통
      "@common/*": ["common/*"],

      //템플릿
      "@template/*": ["template/*"],

      //모듈제외 기능
      "@features/*": ["features/*"],

      //모듈
      "@module/*": ["module/*"],

      //UI-kit
      "@ui-kit/*": ["ui-kit/*"],

      //hooks
      "@hooks/*": ["hooks/*"]
    }
  }
}
